{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "announcements-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/announcements/AnnouncementsWebPart.js",
          "manifest": "./src/webparts/announcements/AnnouncementsWebPart.manifest.json"
        }
      ]
    },
    "promoted-links-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/promotedLinks/PromotedLinksWebPart.js",
          "manifest": "./src/webparts/promotedLinks/PromotedLinksWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "AnnouncementsWebPartStrings": "lib/webparts/announcements/loc/{locale}.js",
    "ControlStrings": "node_modules/@pnp/spfx-controls-react/lib/loc/{locale}.js",
    "PropertyControlStrings": "node_modules/@pnp/spfx-property-controls/lib/loc/{locale}.js",
    "PromotedLinksWebPartStrings": "lib/webparts/promotedLinks/loc/{locale}.js"
  }
}
